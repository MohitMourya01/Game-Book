.project # Eclipse project files
.classpath # Eclipse classpath files
.settings/ # Eclipse settings
.pytest_cache/ # Python pytest cache
.mypy_cache/ # Python mypy cache
.coverage # Python coverage reports
htmlcov/ # Python coverage HTML reports
